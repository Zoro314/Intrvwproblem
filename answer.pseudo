function Namegen(name)

{ 

         if ( typeof name == "string" )              // check if the parameter type is string or not 

        {

         var num = Math.floor(Math.random() * 15) + 6 ;      // generating a random number betweeen 6 and 15 

         return name +" "+num ;                                           // appending the generated random to the name paraameter before retruning it .

        }

        return 1 ;

}
